<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:annotation>
		<xsd:documentation>
			Schema for FINAL REPORTABLE reports about Incidents for EU vigilance exchange.
			version 7.2.1 of 05.2020
		</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="incident">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Section 1: Administrative information -->
				<xsd:element name="admin_info" type="admin_infoT" minOccurs="1" maxOccurs="1"/>
				<!-- Section 2: Medical device information -->
				<xsd:element name="device_info" type="device_infoT" minOccurs="1" maxOccurs="1"/>
				<!-- Section 3: Incident information derived from healthcare professional and/or lay user -->
				<xsd:element name="incident_info" type="incident_infoT" minOccurs="1" maxOccurs="1"/>
				<!-- Section 4: Manufacturer analysis -->
				<xsd:element name="mfr_invest" type="mfrInvestigationT" minOccurs="1" maxOccurs="1"/>
				<!-- Section 5: General comments -->
				<xsd:element name="other_reporting_info" type="other_reporting_infoT" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="version" type="string50T"/>
			<xsd:attribute name="sCreateTimeStamp" type="xsd:dateTime" use="required"/>
			<xsd:attribute name="sFormLanguage" type="isoLanguageT" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!-- SECTION 1: Administrative information -->
	<xsd:complexType name="admin_infoT">
		<xsd:sequence>
			<!-- 1.1 Corresponding competent authority -->
			<xsd:element name="ncaName" type="nonEmptyString200T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="ncaEudamedNum" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ncaReportNo" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="refNumEudamed" type="string50T" minOccurs="0" maxOccurs="1"/>
			<!-- 1.2 Date, type, and classification of incident report -->
			<xsd:element name="reportDate" type="xsd:date" minOccurs="1"/>
			<xsd:element name="adverseEventDateFrom" type="xsd:date" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="adverseEventDateTo" type="xsd:date" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrAwarenessDate" type="xsd:date" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reportType" type="reportTypeT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reportNextDate" type="xsd:date" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="eventClassification" type="eventClassificationT" minOccurs="1" maxOccurs="1"/>
			<!-- 1.3 Submitter information -->
			<!-- 1.3.1 Submitter of the report -->
			<xsd:element name="contact_info" type="contact_infoT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrRef" type="nonEmptyString50T" minOccurs="1"/>
			<xsd:element name="ncaRefMultiDev" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ncaRefMultiDevLI" type="string50T" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="eudamedRefMultiDev" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="eudamedRefMultiDevLI" type="string50T" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="mfrRefMultiDev" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="mfrRefMultiDevLI" type="string50T" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="ncaRefFSCA" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ncaRefFSCALI" type="string50T" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="eudamedRefFSCA" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="eudamedRefFSCALI" type="string50T" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="mfrRefFSCA" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="mfrRefFSCALI" type="string50T" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="psrId" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="psrIdLI" type="string50T" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="pmcfpmpfId" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="pmcfpmpfIdLI" type="string50T" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- 1.3.2	Manufacturer information -->
	<xsd:complexType name="contact_infoT">
		<xsd:choice>
			<xsd:element name="reporterMfr" type="reporterMfrT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterAR" type="reporterART" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="reporterOther" type="reporterOtherT" minOccurs="0" maxOccurs="1"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="reporterMfrT">
		<xsd:sequence>
			<xsd:element name="statusReporter" type="statusReporterMfrT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterDetails" type="reporterDetailsT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrDetails" type="mfrDetailsT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="arDetails" type="authRepDetailsT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="reporterOtherText" type="string50T" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="reporterART">
		<xsd:sequence>
			<xsd:element name="statusReporter" type="statusReporterAuthRepT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterDetails" type="reporterDetailsT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrDetails" type="mfrDetailsT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="arDetails" type="authRepDetailsT" minOccurs="1" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="reporterOtherT">
		<xsd:sequence>
			<xsd:element name="statusReporter" type="statusReporterOtherT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterDetails" type="reporterDetailsT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrDetails" type="mfrDetailsT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="arDetails" type="authRepDetailsT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="reporterOtherText" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="statusReporterMfrT">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Manufacturer"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="statusReporterAuthRepT">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Authorised representative"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="statusReporterOtherT">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Others"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="reporterDetailsT">
		<xsd:sequence>
			<xsd:element name="reporterOrgName" type="nonEmptyString200T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterContactPersonFirstName" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterContactPersonSecondName" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterOrgAddress" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="reporterOrgStreet" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="reporterOrgStreetNum" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="reporterOrgCity" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterOrgCountry" type="isoCountryT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterOrgPOBox" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="reporterOrgPostcode" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterOrgPhone" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="reporterOrgEmailAddress" type="string255T" minOccurs="1" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="mfrDetailsT">
		<xsd:sequence>
			<xsd:element name="mfrName" type="nonEmptyString200T" minOccurs="1"/>
			<xsd:element name="mfrSRN" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="mfrContactPersonFirstName" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrContactPersonSecondName" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrAddress" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="mfrOrgStreet" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="mfrOrgStreetNum" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="mfrCity" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrCountry" type="isoCountryT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrOrgPOBox" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="mfrPostcode" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrPhone" type="nonEmptyString50T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mfrEmailAddress" type="string255T" minOccurs="1" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="authRepDetailsT">
		<xsd:sequence>
			<xsd:element name="ARName" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARSRN" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARContactPersonFirstName" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARContactPersonSecondName" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARAddress" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARStreet" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARStreetNum" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARCity" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARCountry" type="isoCountryT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARPOBox" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARPostCode" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ARPhone" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="AREmailAddress" type="string255T" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Section 2: Medical device information -->
	<xsd:complexType name="device_infoT">
		<xsd:sequence>
			<!-- 2.1 Unique Device Identification (UDI) -->
			<xsd:element name="udiDI" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="udiPI" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="udiDIBasic" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="udiDIUnitUse" type="string50T" minOccurs="0" maxOccurs="1"/>
			<!-- 2.2 Categorisation of device -->
			<xsd:element name="nomenclatureSystem" type="nomenclatureSystemT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="nomenclatureSystemOther" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="nomenclatureCode" type="string50T" minOccurs="0" maxOccurs="1"/>
			<!-- 2.3 Description of device and commercial information -->
			<xsd:element name="brandName" type="string300T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="deviceDescription" type="string4000T" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="modelNum" type="string300T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="catalogNum" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="serialNum" type="string1000T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="batchNum" type="string1000T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceSoftwareVer" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceFirmwareVer" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceMfrDate" type="xsd:date" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceExpiryDate" type="xsd:date" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ImplantedDateFrom" type="xsd:date" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ImplantedDateTo" type="xsd:date" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ExplantedDateFrom" type="xsd:date" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ExplantedDateTo" type="xsd:date" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="implantDuration" type="optDurationT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="implantFacilityName" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="explantFacilityName" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="nbIdNum" type="nbIdNumT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="nbCertNum" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="nbCertNumLI" type="string50T" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="nbIdNum2" type="nbIdNumT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="nbCertNum2" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="nbCertNum2LI" type="string50T" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="deviceMarketDateType" type="marketDateTypeT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceMarketDate" type="optShortStructDateT" minOccurs="0" maxOccurs="1"/>
			<!-- 2.4 Risk class of device when placed on market -->
			<xsd:element name="deviceClass" type="deviceClassT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceClassMDD" type="deviceClassMDDT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceClassIVDD" type="deviceClassIVDDT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceClassMDR" type="deviceClassMDRT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceClassMDRType" type="deviceClassMDRTypeT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceClassIVDR" type="deviceClassIVDRT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceClassIVDRType" type="deviceClassIVDRTypeT" minOccurs="0" maxOccurs="1"/>
			<!-- 2.5 Market distribution of device (region / country) -->
			<xsd:element name="distribution" type="distributionT" minOccurs="1" maxOccurs="1"/>
			<!-- 2.6 Use of accessories, associated devices or other devices -->
			<xsd:element name="deviceAccessories" type="string2000T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceAssociated" type="string2000T" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
  
  <xsd:simpleType name="nbIdNumT">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]+"/>
		</xsd:restriction>
	</xsd:simpleType>
  
	<!-- Section 3: Incident information derived from healthcare professional and/or lay user -->
	<xsd:complexType name="incident_infoT">
		<xsd:sequence>
			<xsd:element name="clinical_event_info" type="clinical_event_infoT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="patient_info" type="patient_infoT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="initial_reporter_info" type="healthcare_facility_infoT" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="clinical_event_infoT">
		<xsd:sequence>
			<!-- 3.1 Nature of incident -->
			<xsd:element name="eventDescription" type="stringT" minOccurs="1" maxOccurs="1"/>
			<!-- 3.2 Medical device problem information -->
			<xsd:element name="imdrfCodeChoice1" type="codeStringT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="imdrfCodeChoice2" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfCodeChoice3" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfCodeChoice4" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfCodeChoice5" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfCodeChoice6" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfCodeMissing" type="string300T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="numPatientsInvolved" type="xsd:integer" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="currentDeviceLocation" type="currentDeviceLocationT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="currentDeviceLocationOtherText" type="string300T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceOperatorAtEvent" type="deviceOperatorAtEventT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceOperatorAtEventOther" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceUsage" type="deviceUsageT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="deviceUsageOther" type="string50T" minOccurs="0"/>
			<xsd:element name="patientRemedialAction" type="stringT" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- 3.3 Patient information -->
	<xsd:complexType name="patient_infoT">
		<xsd:sequence>
			<xsd:element name="imdrfClinicalCodeChoice1" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfClinicalCodeChoice2" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfClinicalCodeChoice3" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfClinicalCodeChoice4" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfClinicalCodeChoice5" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfClinicalCodeChoice6" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfHealthCodeChoice1" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfHealthCodeChoice2" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfHealthCodeChoice3" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfHealthCodeChoice4" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfHealthCodeChoice5" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfHealthCodeChoice6" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfMissingCodeHealthEffect" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="patientAgeAtEvent" type="optDurationT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Gender" type="GenderT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="massKG" type="xsd:decimal" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="patientPriorMedication" type="stringT" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- 3.4 Initial reporter (can be healthcare professional of facility, patient, lay user) -->
	<xsd:complexType name="healthcare_facility_infoT">
		<xsd:sequence>
			<xsd:element name="initialReporterRole" type="initialReporterRoleT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="initialReporterRoleOther" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityName" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityRepNum" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityContactPersonFirstName" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityContactPersonSecondName" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityAddress" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityPhone" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityEmail" type="string255T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityCountry" type="isoCountryT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityStreet" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityStreetNum" type="string200T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityCity" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityPOBox" type="string50T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="healthcareFacilityPostcode" type="string50T" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Section 4: Manufacturer analysis -->
	<xsd:complexType name="mfrInvestigationT">
		<xsd:sequence>
			<!-- 4.1 Manufacturer’s preliminary comments -->
			<xsd:element name="manufacturersPrelimAnalysis" type="stringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="manufacturersInitialCorrecAction" type="stringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="furtherInvestigations" type="stringT" minOccurs="0" maxOccurs="1"/>
			<!-- 4.2 Cause investigation and conclusion -->
			<xsd:element name="rootCauses" type="stringT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="manufacturersWhyNotReportable" type="stringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="rootCauseConfirmed" type="yesNoT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="riskAssReviewed" type="yesNoT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="rationaleNoReview" type="string4000T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="riskAssAdequate" type="yesNoT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="riskAssResults" type="string4000T" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfTypeInvestigationCodeChoice1" type="codeStringT" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="imdrfTypeInvestigationCodeChoice2" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfTypeInvestigationCodeChoice3" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfTypeInvestigationCodeChoice4" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfTypeInvestigationCodeChoice5" type="codeStringT" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="imdrfTypeInvestigationCodeChoice6" type="codeStringT" minOccurs="0" maxOccurs="1"/>
